<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="bpm" package="com.axelor.apps.bpm.db"/>

  <entity name="SurveyResponse">
    <many-to-one name="surveyCampaign" title="Survey Campaign" ref="SurveyCampaign"/>
    <string name="token" unique="true" title="Token" readonly="true"/>
    <string name="email" title="Email"/>
    <boolean name="isCompleted" title="Completed"/>
    <boolean name="isBpm" title="BPM"/>
    <one-to-many name="metaJsonRecordList" title="Forms"
      ref="com.axelor.meta.db.MetaJsonRecord" mappedBy="surveyResponse"/>
  </entity>

</domain-models>
