<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


  <form name="user-form" id="user-form-bpm" title="User" model="com.axelor.auth.db.User"
    extension="true">
    <extend target="//panel[@name='mainPanel']">
      <insert position="inside">
        <field name="supersetUsername"/>
        <field name="supersetPassword" widget="password"/>
      </insert>
    </extend>
  </form>


</object-views>
